{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "rxjs/*": [
        "../node_modules/rxjs/*"
      ],
      "rxjs": [
        "../node_modules/rxjs/"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "module": "esnext",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "./"
  }
}
